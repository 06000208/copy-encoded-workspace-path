        {
          "command": "copyEncodedWorkspacePath"
        }
      ],
      "editor/title/context": [
        {
          "command": "copyEncodedWorkspacePath"
        }
      ]
    }
  },
  "devDependencies": {
    "vscode": "^0.11.0"
  },
  "dependencies": {
    "copy-paste": "^1.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/06000208/vscode-copy-relative-path"
  },
  "bugs": {
    "url": "https://github.com/06000208/vscode-copy-relative-path"
  }
