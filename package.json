{
  "name": "copy-encoded-workspace-path",
  "displayName": "Copy Encoded Workspace Path",
  "description": "Copy a URI Encoded workspace-absolute path from a file",
  "version": "0.0.1",
  "publisher": "06000208",
  "engines": {
    "vscode": "^1.4.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:copyEncodedWorkspacePath"
  ],
  "main": "./extension",
  "contributes": {
    "commands": [
      {
        "command": "copyEncodedWorkspacePath",
        "title": "Copy Encoded Workspace Path"
      }
